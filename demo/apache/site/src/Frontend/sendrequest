const axios = require('axios');

const url = 'http://server_address/endpoint'; // Replace with the actual URL

const data = {
    name: 'test',
    description: 'This is a test JSON for Talis'
};

axios.post(url, data)
  .then(response => {
    console.log('Success:', response.data);
  })
  .catch(error => {
    console.error('Error:', error);
  });
